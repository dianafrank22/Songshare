<body>

<h1> Welcome to Song Share</h1>
<p>Log In to Start Sharing!</p> 
<%= form_tag '/login' do %>
<label>EMAIL</label>
	<input type = 'text' name = 'email' id = 'email'> <br>
	<label>PASSWORD</label>
	<input type = 'password' name= 'password' id= 'password'><br><br>
	<input type = 'hidden' name = 'authenticity_token' value='<%= form_authenticity_token()%>' size = '65'>
	<%= submit_tag 'Log In'%>
<%end%>
<br>
<a href = "/signup">Sign Up</a>
</div>
	



